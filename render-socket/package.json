  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/synth-is/kromosynth-render/issues"
  },
  "homepage": "https://github.com/synth-is/kromosynth-render#readme",
  "dependencies": {
    "kromosynth": "^1.0.35",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.1",
    "node-web-audio-api": "^0.21.3",
    "websocket": "^1.0.34",
    "ws": "^8.14.1"
  },
  "overrides": {
    "virtual-audio-graph": "git+https://github.com/synth-is/virtual-audio-graph.git"
  },
